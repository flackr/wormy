{
  "name": "Wormy",
  "description": "A multiplayer worm game",
  "version": "0.6.5.5",
  "manifest_version": 2,

  "permissions": [
    "experimental",
    {"socket": [
      "tcp-connect",
      "tcp-listen"
    ]},
    "storage"
  ],

  "app": {
    "background": {
      "scripts": ["bin/background.js"]
    }
  },

  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  }
}
